<Application
	x:Class="ApplicationClient.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:views="clr-namespace:ApplicationClient.Views"
	xmlns:viewModels="clr-namespace:ApplicationClient.ViewModels">
	<Application.Resources>
		<DataTemplate DataType="{x:Type viewModels:LoginViewModel}">
			<views:LoginView />
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewModels:RegistrationViewModel}">
			<views:RegistrationView />
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewModels:GeneratorViewModel}">
			<views:GeneratorView />
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewModels:StorageViewModel}">
			<views:StorageView />
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewModels:NavigationSideBarViewModel}">
			<views:NavigationSideBarView />
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewModels:ResetPasswordViewModel}">
			<views:ResetPasswordView />
		</DataTemplate>
	</Application.Resources>
</Application>